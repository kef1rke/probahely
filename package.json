{
	"name": "phely-e-kit",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark"
	},
	"devDependencies": {
		"@mdi/js": "^6.5.95",
		"@smui/button": "^6.0.0-beta.15",
		"@smui/icon-button": "^6.0.0-beta.15",
		"@smui/top-app-bar": "^6.0.0-beta.15",
		"@supabase/supabase-js": "^1.8.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"smui-theme": "^6.0.0-beta.15",
		"svelte": "^3.44.0",
		"svelte-preprocess": "^4.10.1",
		"svelte-time-picker": "^1.0.8",
		"tailwindcss": "^3.0.12"
	},
	"type": "module",
	"dependencies": {
		"@fullcalendar/angular": "^5.10.1",
		"@fullcalendar/core": "^5.10.1",
		"@fullcalendar/daygrid": "^5.10.1",
		"@fullcalendar/list": "^5.10.1",
		"@fullcalendar/timegrid": "^5.10.1",
		"angular": "^1.8.2",
		"cookie": "^0.4.1",
		"daisyui": "^1.21.0",
		"fullcalendar": "^5.10.1",
		"postcss": "^8.4.5",
		"svelte-fullcalendar": "^1.1.2",
		"tw-elements": "^1.0.0-alpha8",
		"typescript": "^4.5.5"
	}
}
